{
  "name": "h3",
  "version": "2.0.0-beta.0",
  "description": "Minimal H(TTP) framework built for high performance and portability.",
  "repository": "unjs/h3",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "bench:bun": "bun run ./test/bench",
    "bench:node": "node --import jiti/register ./test/bench",
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --cache . && prettier -c src test examples docs",
    "lint:fix": "automd && eslint --cache . --fix && prettier -c src test examples docs -w",
    "play:bun": "bun ./test/fixture/bun.ts",
    "play:node": "node --import jiti/register test/fixture/node.ts",
    "play:plain": "node --import jiti/register test/fixture/plain.ts",
    "play:web": "node --import jiti/register test/fixture/web.ts",
    "release": "pnpm test && pnpm build && changelogen --publish --publishTag 2x --prerelease && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest --run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "cookie-es": "^2.0.0",
    "rou3": "^0.5.1",
    "uncrypto": "^0.1.3"
  },
  "resolutions": {
    "h3": "link:."
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.6",
    "autocannon": "^8.0.0",
    "automd": "^0.3.12",
    "changelogen": "^0.5.7",
    "connect": "^3.7.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.20.1",
    "eslint-config-unjs": "^0.4.2",
    "express": "^4.21.2",
    "get-port": "^7.1.0",
    "get-port-please": "^3.1.2",
    "h3-nightly": "npm:h3-nightly@2x",
    "h3-v1": "npm:h3@^1.15.0",
    "jiti": "^2.4.2",
    "listhen": "^1.9.0",
    "mitata": "^1.0.34",
    "prettier": "^3.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.6",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "crossws": "^0.2.4"
  },
  "peerDependenciesMeta": {
    "crossws": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
